@model SOSRequestPortal.Models.Request

@{
    ViewData["Title"] = "Edit";
}

<h2>Editar</h2>

<h4>Requisição</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Editar">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="RequestID" class="control-label"></label>
                <input asp-for="RequestID" class="form-control" />
                <span asp-validation-for="RequestID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Data" class="control-label"></label>
                <input asp-for="Data" class="form-control" />
                <span asp-validation-for="Data" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ticket" class="control-label"></label>
                <input asp-for="Ticket" class="form-control" />
                <span asp-validation-for="Ticket" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Item" class="control-label"></label>
                <input asp-for="Item" class="form-control" />
                <span asp-validation-for="Item" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TipoDeImagem" class="control-label"></label>
                <input asp-for="TipoDeImagem" class="form-control" />
                <span asp-validation-for="TipoDeImagem" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Colaborador" class="control-label"></label>
                <input asp-for="Colaborador" class="form-control" />
                <span asp-validation-for="Colaborador" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Time" class="control-label"></label>
                <input asp-for="Time" class="form-control" />
                <span asp-validation-for="Time" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Gestor" class="control-label"></label>
                <input asp-for="Gestor" class="form-control" />
                <span asp-validation-for="Gestor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Requisitante" class="control-label"></label>
                <input asp-for="Requisitante" class="form-control" />
                <span asp-validation-for="Requisitante" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HorarioDaRequisicao" class="control-label"></label>
                <input asp-for="HorarioDaRequisicao" class="form-control" />
                <span asp-validation-for="HorarioDaRequisicao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HorarioDaRetirada" class="control-label"></label>
                <input asp-for="HorarioDaRetirada" class="form-control" />
                <span asp-validation-for="HorarioDaRetirada" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Patrimonio" class="control-label"></label>
                <input asp-for="Patrimonio" class="form-control" />
                <span asp-validation-for="Patrimonio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NumeroDeSerie" class="control-label"></label>
                <input asp-for="NumeroDeSerie" class="form-control" />
                <span asp-validation-for="NumeroDeSerie" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RetiradoPor" class="control-label"></label>
                <input asp-for="RetiradoPor" class="form-control" />
                <span asp-validation-for="RetiradoPor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EntreguePor" class="control-label"></label>
                <input asp-for="EntreguePor" class="form-control" />
                <span asp-validation-for="EntreguePor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Observacao" class="control-label"></label>
                <input asp-for="Observacao" class="form-control" />
                <span asp-validation-for="Observacao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
	<a asp-action="Index">Voltar para lista de Requisições</a>
</div>

